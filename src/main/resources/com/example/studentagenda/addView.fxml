<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainBox" prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.studentagenda.AddController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Label text="  Add new " />
            <MenuButton fx:id="menuButton" mnemonicParsing="false" text="Class">
              <items>
                <MenuItem mnemonicParsing="false" text="Item" />
                <MenuItem mnemonicParsing="false" text="Other" />
              </items>
            </MenuButton>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
       <AnchorPane VBox.vgrow="ALWAYS">
           <children>
            <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                     <VBox prefHeight="400.0" prefWidth="225.0">
                         <children>
                             <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0" spacing="10.0">
                                 <children>
                                     <Label minWidth="-Infinity" prefWidth="40.0" text="Label" HBox.hgrow="ALWAYS" />
                                     <TextField HBox.hgrow="NEVER" />
                                 </children>
                                 <VBox.margin>
                                     <Insets left="10.0" right="10.0" top="10.0" />
                                 </VBox.margin>
                             </HBox>
                             <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" prefHeight="40.0" prefWidth="200.0" spacing="10.0">
                                 <children>
                                     <Label minWidth="-Infinity" prefWidth="40.0" text="Label" HBox.hgrow="ALWAYS" />
                                     <TextField HBox.hgrow="NEVER" />
                                 </children>
                                 <VBox.margin>
                                     <Insets left="10.0" right="10.0" />
                                 </VBox.margin>
                             </HBox>
                         </children>
                     </VBox>
                     <ListView fx:id="itemList" prefHeight="200.0" prefWidth="265.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="55.0" />
                     </HBox.margin>
                  </ListView>
               </children>
            </HBox>
               <Button mnemonicParsing="false" prefHeight="28.0" prefWidth="80.0" text="Add" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0" />
               <Button mnemonicParsing="false" prefHeight="28.0" prefWidth="80.0" text="Delete" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0" />
           </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
       </AnchorPane>
   </children>
</VBox>
