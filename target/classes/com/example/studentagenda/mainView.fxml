<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.studentagenda.Main">
   <children>
      <AnchorPane minWidth="-Infinity" prefHeight="200.0" prefWidth="70.0" HBox.hgrow="NEVER">
         <children>
            <Button fx:id="addButton" layoutX="25.0" layoutY="755.0" mnemonicParsing="false" onAction="#addButton_Clicked" prefHeight="40.0" prefWidth="40.0" text="add" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="40.0" />
            <Button fx:id="listButton" layoutX="25.0" layoutY="50.0" mnemonicParsing="false" onAction="#listButton_Clicked" prefHeight="40.0" prefWidth="40.0" text="list" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="100.0" />
            <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#settingsButton_Clicked" prefHeight="40.0" prefWidth="40.0" text="set." AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" />
         </children>
      </AnchorPane>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="dateLabel" text="label">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <Pane fx:id="contentPane" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</HBox>
